{
  "name": "mnth",
  "version": "0.1.0-0",
  "description": "Convert date to an array of calendar month",
  "keywords": "date, month, calendar",
  "homepage": "https://github.com/deepsweet/mnth",
  "repository": "deepsweet/mnth",
  "author": "Kir Belevich <kir@belevi.ch> (https://twitter.com/deepsweet)",
  "license": "MIT",
  "files": [
    "build/"
  ],
  "main": "build/node/index.js",
  "browser": "build/browser/index.js",
  "types": "build/index.d.ts",
  "engines": {
    "node": ">=6"
  },
  "peerDependencies": {
    "date-fns": "^2.0.0-0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.46",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-beta.46",
    "@babel/preset-env": "^7.0.0-beta.46",
    "@babel/preset-typescript": "^7.0.0-beta.46",
    "@babel/register": "^7.0.0-beta.46",
    "@start/cli": "^0.1.0-6",
    "@start/plugin-find": "^0.1.0",
    "@start/plugin-lib-babel": "^0.1.0",
    "@start/plugin-lib-codecov": "^0.1.0",
    "@start/plugin-lib-eslint": "^0.1.0",
    "@start/plugin-lib-istanbul": "^0.1.0",
    "@start/plugin-lib-karma": "^0.1.1",
    "@start/plugin-lib-npm-publish": "^0.1.0",
    "@start/plugin-lib-npm-version": "^0.1.0",
    "@start/plugin-lib-tape": "^0.1.0",
    "@start/plugin-lib-typescript-check": "^0.1.0",
    "@start/plugin-lib-typescript-generate": "^0.1.0",
    "@start/plugin-overwrite": "^0.1.0",
    "@start/plugin-parallel": "^0.1.0",
    "@start/plugin-read": "^0.1.0",
    "@start/plugin-remove": "^0.1.0",
    "@start/plugin-rename": "^0.1.0",
    "@start/plugin-sequence": "^0.1.0",
    "@start/plugin-watch": "^0.1.0",
    "@start/plugin-write": "^0.1.1",
    "@start/reporter-verbose": "^0.1.0-2",
    "@types/node": "^10.0.5",
    "babel-loader": "^8.0.0-0",
    "date-fns": "^2.0.0-0",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "esm": "^3.0.34",
    "karma": "^2.0.2",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.1.0",
    "karma-tap": "^4.1.4",
    "karma-tap-pretty-reporter": "^4.0.0",
    "karma-webpack": "^3.0.0",
    "tap-diff": "^0.1.1",
    "typescript": "^2.8.3",
    "typescript-eslint-parser": "^15.0.0",
    "webpack": "^4.8.3"
  },
  "start": {
    "require": [
      "esm",
      [
        "@babel/register",
        {
          "presets": [
            [
              "@babel/preset-env",
              {
                "targets": {
                  "node": "current"
                },
                "modules": false
              }
            ],
            "@babel/preset-typescript"
          ],
          "plugins": [
            "@babel/plugin-syntax-dynamic-import"
          ],
          "extensions": [
            ".ts",
            ".js"
          ]
        }
      ]
    ],
    "reporter": "@start/reporter-verbose"
  },
  "eslintConfig": {
    "parser": "typescript-eslint-parser",
    "plugins": [
      "standard",
      "node",
      "promise"
    ],
    "extends": [
      "standard",
      "plugin:node/recommended",
      "plugin:promise/recommended"
    ],
    "env": {
      "node": true
    },
    "rules": {
      "no-undef": "off",
      "no-unused-vars": "off",
      "prefer-promise-reject-errors": "off",
      "promise/avoid-new": "off",
      "node/no-unsupported-features": [
        "error",
        {
          "ignores": [
            "asyncAwait",
            "modules",
            "restProperties",
            "spreadProperties"
          ]
        }
      ]
    }
  }
}
